{
    "name": "supercharge/supercharge",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/config": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Supercharge\\": "src/"
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Supercharge\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Supercharge\\ServiceProvider"
            ]
        }
    }
}
